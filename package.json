    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "scripts": {
    "prepare": "husky install",
    "transpile": "babel src --out-dir dist --extensions '.ts' --copy-files",
    "tsc": "tsc",
    "clean": "rimraf dist",
    "start": "node dist/index",
    "build": "yarn clean && yarn transpile",
    "babel-node": "babel-node --extensions '.ts'",
    "dev": "nodemon --exec yarn babel-node src",
    "test": "jest",
    "docker:build": "docker build -t babakolo/teamzone . --target dev",
    "docker:test": "docker compose run --rm app yarn test",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev"
  },
  "lint-staged": {
    "**/*.ts": "eslint --fix",
    "**/*": "prettier --write --ignore-unknown"
  },
